  <vType id="ACC_AMax_M" color = 'blue' length="5" maxSpeed="44.1" carFollowModel="IDM" accel="0.6" decel="5.2" tau="2.2" delta="15.5" minGap="6.3" lcAssertive = '5' speedFactor = "normc(1, 0.1, 0.2, 2)"/>
  <vType id="ACC_AMax_R" color = 'white' length="5" maxSpeed="44.1" carFollowModel="IDM" accel="0.6" decel="5.2" tau="2.2" delta="15.5" minGap="6.3" lcAssertive = '1' speedFactor = "normc(1, 0.1, 0.2, 2)"/>


